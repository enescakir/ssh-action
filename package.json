{
  "name": "ssh-runner",
  "version": "1.0.0",
  "description": "Allows you to SSH into a managed Ubicloud runner during workflow execution for debugging purposes",
  "scripts": {
    "build:main": "ncc build -o dist/main --minify --no-cache src/main.js",
    "build:post": "ncc build -o dist/post --minify --no-cache src/post.js",
    "build": "npm run build:main && npm run build:post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubicloud/ssh-runner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ubicloud/ssh-runner/issues"
  },
  "homepage": "https://github.com/ubicloud/ssh-runner#readme",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3"
  }
}
